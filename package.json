{
  "name": "logocn",
  "version": "0.1.0",
  "description": "CLI tool to add SVG logos to your project",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "logocn": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "chmod +x dist/index.js",
    "postinstall": "chmod +x dist/index.js 2>/dev/null || true",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "cli",
    "logos",
    "svg",
    "icons",
    "simple-icons",
    "component-library",
    "shadcn",
    "developer-tools"
  ],
  "author": "LogoCN",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "figlet": "^1.7.0",
    "fs-extra": "^11.3.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^9.2.12",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
