{
  "name": "logocn",
  "version": "0.1.1-beta.7",
  "description": "Add SVG logos to your project with a single command. Like shadcn/ui, but for icons.",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "logocn": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "chmod +x dist/index.js",
    "postinstall": "chmod +x dist/index.js 2>/dev/null || true",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "version": "npm run changelog && git add -A .",
    "postversion": "git push && git push --tags",
    "release:beta": "npm version prerelease --preid=beta && npm publish --tag beta",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "keywords": [
    "cli",
    "svg",
    "logos",
    "icons",
    "react",
    "typescript",
    "shadcn",
    "developer-tools"
  ],
  "author": "@5aikat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shokks/logocn.git"
  },
  "bugs": {
    "url": "https://github.com/shokks/logocn/issues"
  },
  "homepage": "https://github.com/shokks/logocn#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "figlet": "^1.7.0",
    "fs-extra": "^11.3.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^9.2.12",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "auto-changelog": "^2.5.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
